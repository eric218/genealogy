package com.dct.swocean.util;

public enum ResponseEnum
{
    UNKNOW_ERROR(-1, "未知错误"), 
    SUCCESS(200, "请求成功"), 
    ERROR(0, "请求失败"),
    ROLENAME_IS_NULL(201, "角色名为空"),
    KEY_IS_NULL(202, "主键为空")
    ;
    private int code;
    private String msg;

    private ResponseEnum(int code, String msg)
    {
        this.code = code;
        this.msg = msg;
    }

    public int getCode()
    {
        return code;
    }

    public String getMsg()
    {
        return msg;
    }

}
